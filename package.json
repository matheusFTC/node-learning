{
  "name": "node-learning",
  "version": "1.0.0",
  "description": "Curso Do Zero a Produção: Aprenda a Construir uma API Node.js com TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev --transpile-only 'src/index.ts' | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand",
    "validate": "yarn lint:fix && yarn style:fix && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusFTC/node-learning.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Matheus Filipe T. Chaves <matheus.02468@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/matheusFTC/node-learning/issues"
  },
  "homepage": "https://github.com/matheusFTC/node-learning#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.8",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.166",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.10.1",
    "@types/multer": "^1.4.5",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.6",
    "@types/pino": "^6.3.4",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "nock": "^13.0.5",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.1",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.2.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.10.18",
    "pino": "^6.8.0",
    "swagger-ui-express": "^4.1.4"
  }
}
